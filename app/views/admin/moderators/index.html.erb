<h1>Welcome to the admin Panel</h1>

<table class="table" border="1">
  <thead>
    <tr>
      <th>ID</th>
      <th>Full Name</th>
      <th>User Name</th>
      <th>Action</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% @moderators.each do |m| %>
    <tr>
      <td><%= m.id %></td>
      <td><%= m.fullname %></td>
      <td><%= m.username %></td>
      <td><%= link_to 'Edit', edit_admin_moderator_path(m) %></td>
      <td><%= m.created_at.strftime('%A, %b %d') %></td>
    </tr>
    <% end %>
  </tbody>
</table>